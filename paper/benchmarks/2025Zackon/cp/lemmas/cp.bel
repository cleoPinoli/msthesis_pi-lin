    let Prune-Oft [_,x:obj ‚ä¢ O1'] [_,x:obj,y:obj ‚ä¢ _] = prune_oft [_,x:obj,y:obj ‚ä¢ O1[..,y,x]] in
    let Prune-Oft [_,x:obj ‚ä¢ O2'] [_,x:obj,y:obj ‚ä¢ _] = prune_oft [_,x:obj,y:obj ‚ä¢ O2[..,y,x]] in
